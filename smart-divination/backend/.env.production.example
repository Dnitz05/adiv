# Production Environment Variables
# Copy to .env.production and fill with real values

# Supabase Configuration (REQUIRED)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# DeepSeek AI (REQUIRED for interpretations)
DEEPSEEK_API_KEY=your_production_deepseek_key

# Random.org (OPTIONAL - signed randomness)
RANDOM_ORG_KEY=your_production_random_org_key

# Feature Flags (REQUIRED)
ENABLE_ICHING=false
ENABLE_RUNES=false

# Metrics & Observability (OPTIONAL)
METRICS_PROVIDER=datadog
METRICS_DEBUG=false
DATADOG_API_KEY=your_datadog_api_key
DATADOG_SITE=datadoghq.com
DATADOG_SERVICE=smart-divination-backend
DATADOG_ENV=production
DATADOG_METRIC_PREFIX=smart_divination
DATADOG_TAGS=env:production,service:backend
DATADOG_TIMEOUT_MS=2000

# API Configuration
NODE_ENV=production

# IMPORTANT: Never commit this file with real values!
# Add .env.production to .gitignore
